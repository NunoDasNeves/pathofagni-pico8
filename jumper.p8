--thang - entity/actor

thang = {}

-- number of layers to draw
max_z = 0

function init_thang_dat()
thang_dat = {
	[82] = { -- lantern
		lit = false,
		update = update_lantern,
		burn = burn_lantern,
		replace = 82 + 3,
		z = 1
	},
	[96] = { -- bat
		update = update_bat,
		burn = burn_bat,
		rght = true,
		w = 7,
		h = 7,
		alive = true
	}
}
end

-- spawn thangs in current room
-- save room
function spawn_room()
 local rmapx = room.x \ 8
 local rmapy = room.y \ 8
 local rmapr=rmapx+15
 local rmapb=rmapy+15
 thang = {}
 max_z = 0
 room.old = {}
 for y=rmapy,rmapb do
  for x=rmapx,rmapr do
   local val = mget(x,y)
   if (fget(val,4)) then
    add(room.old, {x=x,y=y,val=val})
   	local t = {}
   	t.x = x*8
   	t.y = y*8
   	t.s = val
   	t.fr = 0
    t.fcnt = 0
    t.draw = draw_thang
    t.replace = 0
    t.w = 8
    t.h = 8
    t.z = 0
    for k,v in pairs(thang_dat[val]) do
    	t[k] = v
    end
				max_z = max(t.z, max_z)
    add(thang,t)
    mset(x,y,t.replace)
   end
  end
 end
end

