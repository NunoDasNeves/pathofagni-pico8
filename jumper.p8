-- player

p_dat = {
	i = 64, -- base of sprite row
	--  animations - s = offset from spr, f = num frames
	s_wlk =  {s=0, f=2},
	s_jmp =  {s=2, f=5},
	s_die =  {s=7, f=5},
	s_spwn = {s=12, f=4},
	w = 8,
	h = 8,
	
	--  physics
	--  coll dimensions
	ftw = 0.99, -- < min platform width
	ftx = 3,
	ch = 6.99, -- < min wall height
	cw = 5.99,
	cx = 1,
	cy = 1,
	-- hurtbox dimensions
	hx = 2,
	hy = 2,
	hw = 3.99,
	hh = 3.99,
	-- physics
	ax = 1, -- accel
	max_vx = 2,
	min_vx = 0.01, -- stop threshold
	g = 0.5, -- gravity
	max_vy = 5,
	j_vy = -6, -- jump accel
}

function spawn_p(x,y)

 p = {}
	p.x = x
	p.y = y
	p.rght = true -- facing
	p.vx = 0
	p.vy = 0
	p.air = false
	p.fr = 0 -- displayed frame offset
	p.fcnt = 0 -- counter for advancing frame
	p.shcount = 0 -- shoot counter
	p.teeter = false
	p.alive = true
	p.spawn = true
	for k,v in pairs(p_dat) do
		p[k] = v
	end
	p.s = p.i + p.s_spwn.s

end

fireball = {}

function make_fireball()
 local f = {}
 f.w = 4
 f.h = 4
 f.x = p.x + (p.w - f.w)/2
 f.y = p.y + (p.h - f.h)/2
 f.s = 80
 f.alive = true
 f.fcnt = 0
 f.speed = 3
 f.frame = 0
 local ydir = 0
 local xdir = 0
 if (btn(â¬†ï¸)) then
  ydir = -1
 elseif (btn(â¬‡ï¸)) then
  ydir = 1
 end
 if (p.rght) then
  xdir = 1
 else
  xdir = -1
 end
 -- straight up or down
 if (not btn(â¬…ï¸) and
     not btn(â¡ï¸) and
     ydir != 0) then
   xdir = 0
 end
 if (xdir == 0 or ydir == 0) then
  f.vx = xdir * f.speed
  f.vy = ydir * f.speed
 else
  f.vx = xdir * 0.7071 * f.speed
  f.vy = ydir * 0.7071 * f.speed
 end

 f.sframe = 0 -- sub-frame
	if (ydir == 0) then
  f.sframe = 1
 elseif (xdir == 0) then
  f.sframe = 2
 else
  f.sframe = 3
 end
 f.xflip = false
 f.yflip = false
 if (f.vy < 0) then
  f.yflip = true
 end
 if (f.vx < 0) then
  f.xflip = true
 end
 add(fireball, f)
end

function kill_fireball(f)
	f.alive = false
	f.yflip = false
	f.sframe = 0
	f.frame = 1
end

function update_fireball(f)
 if (not f.alive) then
  f.y -= 0.5
	 f.fcnt += 1
	 if (f.fcnt & 1 == 0) then
	 	f.sframe += 1
	 end
	 if (f.fcnt == 8) then
		 del(fireball, f)
		end
  return
 end
 f.x += f.vx
 f.y += f.vy
 -- hit stuff
 for t in all(thang) do
 	if (aabb(
 									 t.x,t.y,t.w,t.h,
 									 f.x,f.y,4,4)) then
 	 t:burn()
 	 -- don't stop on lanterns
 	 if (t.s != 82) then
	 	 kill_fireball(f)
	 	 return
 		end
 	end
 end
 if (--collmap(f.x,f.y,0) or
     collmap(f.x,f.y,1) or
     collmap(f.x,f.y,2)) then
 	f.vx = 0
 	f.vy = 0
 	kill_fireball(f)
 end
end

function kill_p()
 p.alive = false
 p.s = p.i + p.s_die.s 
 p.fr = 0
 p.fcnt = 0
end

function update_p()
 if (p.spawn or not p.alive) then
  respawn_update_p()
  return
 end

 -- change direction
 if (btnp(â¬…ï¸) or
     btn(â¬…ï¸) and not btn(â¡ï¸)) then
  p.rght = false
 elseif (btnp(â¡ï¸) or
     btn(â¡ï¸) and not btn(â¬…ï¸)) then
  p.rght = true
 end
 if (btn(â¬…ï¸) and not p.rght) then
 	-- accel left
 	p.vx -= p.ax
 elseif (btn(â¡ï¸) and p.rght) then
 	-- accel right
 	p.vx += p.ax
 end
 if (not btn(â¬…ï¸) and not btn(â¡ï¸)) then
  p.vx -= p.vx/3
 end
 p.vx = clamp(p.vx, -p.max_vx, p.max_vx)
 if (abs(p.vx) < p.min_vx) then
  p.vx = 0
 end

 local newx = p.x + p.vx

 local cl = newx + p.cx
 local cr = cl + p.cw
 local ct = p.y + p.cy
 local cb = ct + p.ch
 -- only check left or right
 local cx = cr
 if (p.vx < 0) then
  cx = cl
 end
 -- collide walls
 if ((p.vx != 0)
     and
     (collmap(cx,ct,1) or
 	    collmap(cx,cb,1))
 	  ) then
   p.vx = 0
   -- push out of wall
   if (cx == cl) then
   	newx = roundup(cx, 8) - p.cx - 1
   else
   	newx = rounddown(cx, 8) - p.w + p.cx + 1
   end
 end

 -- vy - jump and land
 local oldair = p.air
	if (btnp(ğŸ…¾ï¸) and not p.air) then
		 p.vy += p.j_vy
		 p.air = true
	end
	p.vy += p.g
	p.vy = clamp(p.vy, -p.max_vy, p.max_vy)

 local newy = p.y + p.vy
 local fty = newy + p.h
 local ftxl = newx + p.ftx
 local ftxr = ftxl + p.ftw

 if (p.vy > 0) then
  if ((collmap(ftxl,fty,0) or
			   collmap(ftxr,fty,0))
      and
      -- need both checks!
      (not p.air or
      rounddown(p.y,8) < rounddown(newy,8))) then
			newy = rounddown(newy, 8)
	  p.vy = 0
	  p.air = false
	 else
	  -- fall off platform
	  if (not p.air) then
	   -- only fall if holding dir
	   if ((btn(â¬…ï¸) and p.vx < 0) or
	       (btn(â¡ï¸) and p.vx > 0)) then
	   	p.air = true
	   else
	    p.vx = 0
	    newx = p.x
	   end
		 end
	 end
	 -- teeter
	 local tftxl = newx + p.ftx
	 local tftxr = tftxl + p.ftw
	 if (not (collmap(tftxl-1,fty,0) and
			   collmap(tftxr+1,fty,0))
			   and not p.air) then
		 p.teeter = true
		else
		 p.teeter = false
		end
	-- p.vy < 0
 else
  -- ceiling
 	if (p.air and (collmap(ftxl,newy,2) or
 	    collmap(ftxr,newy,2))) then
 	 p.vy = -p.vy/3
 	end
 end

 p.x = newx
 p.y = newy
 
 -- hit spikes
 local hl = p.x + p.hx
 local hr = hl + p.hw
 local ht = p.y + p.hy
 local hb = ht + p.hh
 if (collmap(hl,ht,3) or
     collmap(hr,ht,3) or
     collmap(hl,hb,3) or
     collmap(hr,hb,3)) then
 	kill_p()
 	respawn_update_p()
 	return
 end
 
 if (p.shcount == 0) then
  if (btnp(â)) then
   p.shcount = 10
   make_fireball()
  end
 else
 	p.shcount -= 1
 end
 
 -- animate
 if (not p.air) then
  -- walk anim
  p.s = p.i + p.s_wlk.s
  if (btnp(â¡ï¸) or btnp(â¬…ï¸)) then
   p.fr = 0
  end
  if (btn(â¡ï¸) or btn(â¬…ï¸)) then
  	if (p.fcnt > 2) then
	 		p.fr = (p.fr + 1) % p.s_wlk.f
	 		p.fcnt = 0
	 	end
	 elseif (p.teeter) then
	  p.fr = 1
	 else
	  p.fr = 0
	 end
 else
  if (not oldair) then
 		 -- start jump anim
		 p.s = p.i + p.s_jmp.s
   p.fr = 0
   p.fcnt = 0
   -- fell, not jumped
   if (not btn(ğŸ…¾ï¸)) then
   	p.fr = 3
   end
  end
  -- jump anim
  if (p.fcnt > 2) then
			p.fr += 1
			-- loop last 2 frames
			if (p.fr >= p.s_jmp.f) then
			 p.fr -= 2
			end
			p.fcnt = 0
 	end
 end
 p.fcnt += 1
end

function respawn_update_p()
	if (not p.alive) then
		if (p.fcnt >= 2) then
   p.fr += 1
   p.fcnt = 0
  end
	 if (p.fr >= p.s_die.f) then
	  -- todo update room
   spawn_p(curr_lantern.x,curr_lantern.y - p.h)
  end

 elseif (p.spawn) then
  if (p.fcnt >= 3) then
   p.fr += 1
   p.fcnt = 0
  end
  if (p.fr >= p.s_spwn.f) then
   p.fr = 0
   p.fcnt = 0
   p.s = p.i + p.s_wlk.s
   p.spawn = false
  end
	end

	p.fcnt += 1
end
