-- jumper

#include util.lua

-- 
dbg = true
dbgstr = ''

-- disable btnp repeating
poke(0x5f5c, 255)

room = {
 i = 0,
 x = 0,
 y = 0,
 sz = 16*8,
 old = {}, -- for restore
 fcnt = 0,
 max_i = 8*3 - 1
}

-- one lantern per room
-- for now
-- room index+1 -> lit
lantern = {}
curr_lantern = {}

function init_lantern()
 -- todo this properly
 for i=0,room.max_i do
  add(lantern, false)
 end
 lantern[1] = true
 curr_lantern.x = 64
 curr_lantern.y = 64
end

function burn_bat(b)
 if (b.alive) then
	 b.alive = false
	 b.s += 2
	 b.vy = 0.6
	 b.deadf = 20
	end
end

function update_bat(b)
 if (not b.alive) then
  b.deadf -= 1
  if (b.deadf == 0) then
   del(thang, b)
  end
 end
 -- animate
	b.fcnt += 1
	if (b.fcnt == 4) then
	 b.fr += 1
	 if (b.fr > 1) then
	  b.fr = 0
	 end
	 b.fcnt = 0
	end
	
	if (b.alive) then
		-- follow player
		-- todo un-jank
		if (b.x - p.x > 4) then
	  b.rght = false
	  b.vx = -0.4
	 elseif (b.x - p.x < -4) then
	  b.rght = true
	  b.vx = 0.4
	 end
	 if (b.y - p.y > 0) then
	  b.vy = -0.3
	 else
	  b.vy = 0.3
	 end
	
		-- bounce
		-- todo un-jank
		if (b.fr == 0) then
		 b.vy += 0.5
		else
	  b.vy -= 0.5
	 end
 end
	
 b.x += b.vx
 b.y += b.vy
end

function burn_lantern(l)
 if (not l.lit) then
	 l.lit = true
 	l.fr = 1
 	lantern[room.i+1] = true
 end
end

function update_lantern(l)
 if (l.lit) then
  l.fcnt += 1
  if (l.fcnt == 5) then
   l.fr += 1
   l.fcnt = 0
  end
  if (l.fr > 2) then
   l.fr = 1
  end
 end
end

function collmap(x,y,f)
 local val = mget(x\8,y\8)

 return (fget(val,f))
end

function restore_room()
 for t in all(room.old) do
  mset(t.x,t.y,t.val)
 end
end

function room_update()
 local px = p.x + p.w/2
 local py = p.y + p.h/2
 -- todo absolute
 if (px <= room.x) then
  room.i -= 1
  room.x -= room.sz
 elseif (px > room.x + room.sz) then
  room.i += 1
  room.x += room.sz
 elseif (py <= room.y) then
  room.y -= room.sz
  room.i -= 8
 elseif (py > room.y + room.sz) then
  room.y += room.sz
  room.i += 8
 end
end

function _update()
 local oldi = room.i
 room_update()
 if (oldi != room.i) then
  camera(room.x, room.y)
  fireball = {}
  restore_room()
  spawn_room()
 end
 dbgstr = ''
	update_p()
	for t in all(thang) do
	 t:update()
	end
	foreach(fireball, update_fireball)
end

function _init()
	camera(0,0)
 init_thang_dat()
 init_lantern()
	spawn_room()
	spawn_p(curr_lantern.x,curr_lantern.y - p_dat.h)
end
